{
    "name": "nhzex/think-blade",
    "description": "Use Laravel Blade templates without the full Laravel framework",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "symfony/debug": "^4.2",
        "symfony/finder": "^4.2",
        "topthink/framework": "6.0.x-dev",
        "topthink/think-helper": "^3.1.1"
    },
    "require-dev": {
        "doctrine/inflector": "^1.1",
        "phpunit/phpunit": "^8",
        "ramsey/uuid": "^3.7",
        "symfony/var-dumper": "^4.3",
        "topthink/think-template": "^2.0"
    },
    "suggest": {
        "ramsey/uuid": "Required to use Str::uuid() (^3.7).",
        "symfony/var-dumper": "Required to use the dd function (^4.2).",
        "doctrine/inflector": "Required to use the pluralizer (^1.1)."
    },
    "autoload": {
        "files": [
            "src/helper.php",
            "lib/support/helpers.php"
        ],
        "psr-4": {
            "HZEX\\Blade\\": "src/",
            "Illuminate\\Container\\": "lib/container",
            "Illuminate\\Contracts\\": "lib/contracts",
            "Illuminate\\Support\\": "lib/support",
            "Illuminate\\Filesystem\\": "lib/filesystem",
            "Illuminate\\View\\": "lib/view"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\BladeTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
