{
    "name": "nhzex/think-blade",
    "description": "Use Laravel Blade templates without the full Laravel framework",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "duncan3dc/blade": "^4.5",
        "topthink/think-helper": "^3.1.1",
        "topthink/framework": "6.0.x-dev",
        "topthink/think-view": "master-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "HZEX\\Blade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\BladeTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
