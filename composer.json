{
    "name": "nhzex/think-blade",
    "description": "Use Laravel Blade templates without the full Laravel framework",
    "keywords": [
        "thinkphp",
        "thinkphp6",
        "blade"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/finder": "^5.4|^6.0",
        "topthink/framework": "~6.0.0",
        "topthink/think-helper": "^3.1.1"
    },
    "require-dev": {
        "doctrine/inflector": "^1.4|^2.0",
        "phpunit/phpunit": "^7|^8|^9",
        "symfony/var-dumper": "^5.0",
        "topthink/think-template": "^2.0",
        "voku/portable-ascii": "^1.4.8",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "ramsey/uuid": "Required to use Str::uuid() (^3.7).",
        "symfony/var-dumper": "Required to use the dd function (^4.2).",
        "doctrine/inflector": "Required to use the pluralizer (^1.4|^2.0).",
        "symfony/mime": "Required to enable support for guessing extensions (^5.0).",
        "voku/portable-ascii": "Required to enable support for string ascii extensions (^1.4.8)."
    },
    "autoload": {
        "files": [
            "src/helper.php",
            "lib/support/helpers.php"
        ],
        "psr-4": {
            "think\\view\\driver\\": "driver",
            "HZEX\\Blade\\": "src",
            "Illuminate\\Container\\": "lib/container",
            "Illuminate\\Contracts\\": "lib/contracts",
            "Illuminate\\Support\\": [
                "lib/support",
                "lib/collections",
                "lib/macroable"
            ],
            "Illuminate\\Filesystem\\": "lib/filesystem",
            "Illuminate\\View\\": "lib/view"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HZEX\\BladeTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": false
    }
}
